  "query": "\n        SELECT ea.email_address_uid, ea.email_address, ea.verified_at\n        FROM email_address ea\n        JOIN account a ON ea.id = a.email_address_id\n        WHERE ea.active IS TRUE\n        AND a.account_uid = $1\n        ",
