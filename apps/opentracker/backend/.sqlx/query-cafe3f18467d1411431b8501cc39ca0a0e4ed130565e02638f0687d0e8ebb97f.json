  "query": "\n        SELECT a.account_uid, ea.email_address, a.password, a.created_at\n        FROM account a\n        JOIN email_address ea ON a.email_address_id = ea.id\n        WHERE a.account_uid = $1\n        AND ea.active = TRUE\n        ",
