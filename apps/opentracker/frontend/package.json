{
  "name": "opentracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@reduxjs/toolkit": "^2.11.2",
    "@tanstack/react-query": "4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "14",
    "@testing-library/user-event": "13",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.6",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.13.2",
    "babel-jest": "^30.2.0",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.1.0",
    "bfj": "^9.1.2",
    "browserslist": "^4.24.5",
    "camelcase": "^9.0.0",
    "chart.js": "^4.5.1",
    "chartjs-adapter-luxon": "^1.3.1",
    "eslint": "^9.39.2",
    "eslint-config-react-app": "^7.0.1",
    "fs-extra": "^11.3.3",
    "fuse.js": "^7.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-resolve": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "luxon": "^3.7.2",
    "postcss": "^8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^13.0.1",
    "postcss-preset-env": "^10.6.1",
    "prompts": "^2.4.2",
    "react": "18",
    "react-app-polyfill": "^3.0.0",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "18",
    "react-hook-form": "^7.71.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.12.0",
    "resolve": "^1.22.11",
    "semver": "^7.7.3",
    "typescript": "^5.9.3",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "start": "rsbuild dev",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not op_mob >= 1"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rsbuild/core": "^1.7.2",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rsbuild/plugin-svgr": "^1.2.3",
    "@types/redux-mock-store": "^1.5.0",
    "axios-mock-adapter": "^2.1.0",
    "history": "^5.3.0",
    "jest-environment-jsdom": "^30.2.0",
    "msw": "0.36.8",
    "prettier": "^3.7.4",
    "redux-mock-store": "^1.5.5",
    "web-streams-polyfill": "^4.2.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "<rootDir>/tests/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!chartjs-adapter-luxon).+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^chartjs-adapter-luxon$": "<rootDir>/node_modules/chartjs-adapter-luxon/dist/chartjs-adapter-luxon.umd.js",
      "~/(.*)": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "runtime": "automatic"
        }
      ]
    ]
  }
}
